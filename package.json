  "scripts": {
    "build": "npm run getBlog && gatsby build",
    "develop": "gatsby develop",
    "serve": "npm run build && gatsby serve",
    "format": "prettier --write 'src/**/*.js'",
    "test": "echo \"No test specified\"",
    "getBlog": "npm run zeroBlog && npm run cloneBlog",
    "cloneBlog": "export $(egrep -v '^#' .env | xargs) && git clone \"git@github.com:$BLOG_REPO\" data/blog",
    "zeroBlog": "rm -rf data/blog",
    "pullBlog": "git --work-tree=data/blog --git-dir=data/blog/.git pull"
